/* pages/myPrize/myPrize.wxss */
.container{
    width:750rpx;
    box-sizing:border-box;
    position:relative;
}
.myPrize-banner{
    height:340rpx;
    background: #FF2D73;
    position:relative;
}
.myPrize-banner:after{
    content:'';
    display:block;
    width:750rpx;
    height:63rpx;
    position:absolute;
    bottom:0;
    background:url(data:image/png;base64,) no-repeat;
    background-size:100% 100%;
}
.myPrize{
    text-align:center;
    padding:20rpx;
    -webkit-border-radius:20rpx;
    border-radius:20rpx;
    margin:0 30rpx 30rpx;
    background:#fff;
    margin-top:-310rpx;
    position:relative;
    z-index:9;
}
.noAward{
    height:515rpx;
}
.noAward-pic{
    display:block;
    width:60rpx;
    height:60rpx;
    margin:120rpx auto 30rpx;
}
.noAward-text{
    font-size:28rpx;
    color:#ccc;
}
.noAward-time{
    font-size:56rpx;
    color:#333;
    margin-top:38rpx;
    line-height:70rpx;
    display:block;
}
.noAward-time-txt{
    font-size:36rpx;
}
.winAward{
    padding-top:38rpx;
    margin-bottom:70rpx;
}
.mypet-avatar{
    width:120rpx;
    height:120rpx;
    border: 1px solid #E8BC7F;
    -webkit-border-radius:100%;
    border-radius:100%;
    position:relative;
}
.mypet-num{
    font-size:26rpx;
    color:#C48A38;
    width:265rpx;
    line-height:53rpx;
    background: #FFF9D8;
    border: 1px solid #E8BC7F;
    -webkit-border-radius: 30rpx;
    border-radius: 30rpx;
    margin:-17rpx auto 0;
    position:relative;
    z-index:9;
}
.mypet-name{
    font-size:34rpx;
    color:#333;
    line-height:48rpx;
    margin:10rpx 0 48rpx;
}
.mypet-win{
    font-size:32rpx;
    color:#C48A38;
}
.mypet-tip{
    font-size:28rpx;
    color:#999;
    line-height:40rpx;
    margin:14rpx 0 48rpx;
}
.mypet-prize{
    font-size:52rpx;
    color:#333;
    line-height:73rpx;
}
.mypet-prize-text{
    display:inline;
    font-size:50rpx;
}
.mypet-cash{
    width:598rpx;
    height:366rpx;
    padding-top:38rpx;
    background:url(https://static.58.com/lbg/mengchong/image/packet.png) no-repeat;
    background-size:100% 100%;
    margin:42rpx auto 58rpx;
}
.mypet-cash-num{
    font-size:56rpx;
    color:#FF552E;
    line-height:80rpx;
}
.mypet-cash-packet{
    font-size:28rpx;
    color:#333;
    line-height:45rpx;
}
.mypet-reward{
    font-size:26rpx;
    color:#ccc;
}
.noPrize{
    height:515rpx;
    margin-top:-310rpx;
    position:relative;
    z-index:9;
    text-align:center;
    padding:20rpx;
    -webkit-border-radius:20rpx;
    border-radius:20rpx;
    background:#fff;
}
.noPrize-pic{
    display:block;
    width:60rpx;
    height:60rpx;
    margin:150rpx auto 30rpx;
}
.noPrize-txt{
    font-size:28rpx;
    color:#ccc;
}