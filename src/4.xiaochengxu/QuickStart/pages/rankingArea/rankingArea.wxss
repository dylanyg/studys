/* pages/rankingArea/rankingArea.wxss */
.body{
    background:#F6F6F6;
}
.container{
    width:750rpx;
    box-sizing:border-box;
    position:relative;
}
.rankingArea{
    text-align:center;
    padding:20rpx;
    -webkit-border-radius:20rpx;
    border-radius:20rpx;
    margin:0 30rpx 30rpx;
    background:#fff;
}
.rankingArea-banner{
    height:340rpx;
    background: #FF2D73;
    position:relative;
}
.rankingArea-banner:after{
    content:'';
    display:block;
    width:750rpx;
    height:63rpx;
    position:absolute;
    bottom:0;
    background:url(data:image/png;base64,) no-repeat;
    background-size:100% 100%;
}
.ranking-mypet{
    height:515rpx;
    margin-top:-310rpx;
    position:relative;
    z-index:9;
}
.ranking-mypet-avatar{
    height:120rpx;
    width:120rpx;
    -webkit-border-radius:100%;
    border-radius:100%;
    border:1rpx solid #E8BC7F;
}
.ranking-mypet-name{
    font-size:34rpx;
    color:#333;
    line-height:48rpx;
    margin:10rpx 0 36rpx;
    display:block;
}
.ranking-mypet-reveal{
    display: -webkit-flex; 
    display: flex;
}
.ranking-mypet-reveal-detail{
    -webkit-flex:1;
    flex:1;
    color:#666;
    border-right:1rpx solid #F3F3F3;
}
.ranking-mypet-reveal-detail.myVote{
    border-right:none;
}
.reveal-detail-first{
    font-size:34rpx;
    line-height:48rpx;
    margin-bottom:5rpx;
}
.reveal-detail-last{
    font-size:26rpx;
    line-height:37rpx;
}
.ranking-mypet-attend{
    font-size:34rpx;
    color:#fff;
    width:400rpx;
    line-height:88rpx;
    background:#FF552E;
    -webkit-border-radius:50rpx;
    border-radius:50rpx;
    margin:80rpx auto 0;
}
.ranking-mypet-nonepic{
    display:block;
    width:60rpx;
    height:60rpx;
    margin:120rpx auto 30rpx;
}
.ranking-mypet-nonetext{
    font-size:28rpx;
    color:#ccc;
}
.ranking-mypet-top{
    margin-top:130rpx;
}
.rankingArea-list{
    background:#fff;
}
.rankingArea-list-reveal{
    height:110rpx;
    padding:20rpx 30rpx;
    box-sizing:border-box;
    display:-webkit-flex;
    display:flex;
    border-bottom:1rpx solid #F3F3F3;
}
.rankingArea-list-award{
    width:100rpx;
    position:relative;
}
.rankingArea-list-award:after{
    content:'';
    display:block;
    width:44rpx;
    height:56rpx;
    position:absolute;
    left:28rpx;
    top:7rpx;
}
.rankingArea-list-award-txt{
    font-size:34rpx;
    color:#666;
    display:block;
    text-align:center;
    line-height:70rpx;
}
.num1:after{
    background:url(data:image/png;base64,) no-repeat;
    background-size:100% 100%;
}
.num2:after{
    background:url(data:image/png;base64,) no-repeat;
    background-size:100% 100%;
}
.num3:after{
    background:url(data:image/png;base64,) no-repeat;
    background-size:100% 100%;
}
.rankingArea-list-avatar{
    -webkit-flex:2;
    flex:2;
    width:100rpx;
}
.rankingArea-list-avatar-img{
    display:block;
    width:70rpx;
    height:70rpx;
    border:1rpx solid #F3F3F3;
    -webkit-border-radius:100%;
    border-radius:100%;
}
.rankingArea-list-name{
    font-size:34rpx;
    color:#333;
    line-height:70rpx;
    width:430rpx;
    -webkit-flex:4;
    flex:4;
}
.rankingArea-list-name-txt{
    font-size:34rpx;
    color:#333;
    display:block;
    line-height:70rpx;
}
.rankingArea-list-praise{
    width:60rpx;
    -webkit-flex:3;
    flex:3;
}
.rankingArea-list-praise-myVote{
    font-size:26rpx;
    line-height:35rpx;
    text-align:center;
    color:#333;
}
.rankingArea-list-praise-txt{
    font-size:26rpx;
    line-height:35rpx;
    text-align:center;
    color:#999;
}