/* 分享、点赞 */
.petBox{
    width:100%;
    height:92rpx;
    line-height:92rpx;
    display:flex;
    justify-content:space-between;
    padding:0 30rpx;
    box-sizing:border-box;
    background:#fff;
}
.petNickName{
    flex:1;
    font-size: 34rpx;
    color: #333333;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}
.focusNum{
    text-align:right;
    font-size: 26rpx;
    color: #666666;
    width:120rpx;
}
.petDo{
    border-top:1rpx solid #f3f3f3;
}
.doWithPet{
    flex:1;
}
.doItem::before{
    content:"";
    width:46rpx;
    height:46rpx;
    display:inline-block;
    background:url(data:image/png;base64,);
    background-size:46rpx 46rpx;
    margin:0 10rpx -10rpx;
}
.dianzan::before{
    width:46rpx;
    height:46rpx;
    display:inline-block;
    background:url(data:image/png;base64,);
    background-size:46rpx 46rpx;
    margin:0 10rpx -10rpx;
}
.doItem.jubao{
    text-align:left;
}
.jubao::before{
    background:url(data:image/png;base64,);
    background-size:46rpx 46rpx;
}
.doItem.share::before{
    background:url(data:image/png;base64,);
        background-size:46rpx 46rpx;
        margin:0 10rpx -10rpx;
}
.doItem.share{
    text-align:right;
}
.doItem{
    display:inline-block;
    line-height:92rpx;
    width:33.3%;
    box-sizing:border-box;
    margin:0 auto;
    text-align:center;
    font-size: 26rpx;
    color: #333333;
    background:#fff;
    position:static;
    padding:0;
}
.doItem::after{
    width:0;
    height:0;
    position:static;
    border:0;
}




/* 宠物身份 */
.petCard{
    width:100%;
    height:228rpx;
    margin-top:20rpx;
    background:#fff;
}
.petCard-header{
    height:76rpx;
    line-height:76rpx;
    padding-left:30rpx;
    font-size: 26rpx;
    color: #999999;
}
.petCard-body{
    display:flex;
    border-top:1rpx solid #f3f3f3;
    position:relative;
    padding:20rpx 30rpx 0 30rpx;
}
.petCardAvatar{
    width:100rpx;
    height:100rpx;
    border-radius:50%;
    background:#000;
    margin-right:20rpx;
    margin-top:5rpx;
}
.petCardInfo{
    width:390rpx;
    overflow:hidden;
}
.petCardName{
    font-size: 34rpx;
    color: #333333;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}
.petCardDetail{
    display:flex;
    line-height:70rpx;
}
.petCardDetail-Item{
    flex:1;
    font-size: 26rpx;
    color: #999999;
}
.toFocus{
    width:160rpx;
    height:68rpx;
    text-align:center;
    line-height:68rpx;
    background:#FF552E;
    font-size: 28rpx;
    color: #FFFFFF;
    position:absolute;
    right:30rpx;
    top:40rpx;
}
.toFocus.focused{
    background:#f3f3f3;
    color: #999999;
}



/* 热门推荐 */

.noHotTop{
    width:100%;
    height:230rpx;
    color:#888;
    font-size:32rpx;
    text-align:center;
    line-height:230rpx;
}
.hotTop{
    width:100%;
    flex:1;
    margin-top:20rpx;
    background: #FFFFFF;
}
.hotTop-header{
    height:108rpx;
    line-height:108rpx;
    text-align:center;
    font-size: 34rpx;
    color: #333333;
}
.hotTop-body{
    width:100%;
    display:flex;
    flex-wrap:wrap;
    justify-content:space-between;
}
.hotTop-Item{
    width:372rpx;
    margin-bottom:40rpx;
}
.itemAvatar{
    width:100%;
    height:210rpx;
    background:#000;
}
.petInfo{
    padding:0 20rpx;
    
}
.content{
    font-size: 28rpx;
    padding-top:19rpx;
    color:#333;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    width:100%;
    box-sizing:border-box;
}
.focusInfo{
    padding:10rpx 0 20rpx 0;
    display:flex;
    justify-content:space-between;
}
.playNum,.focusTime{
    flex:1;
    color:#999;
    font-size: 24rpx;
}

.focusTime{
    text-align:right;
}
.focusTime::before,.playNum::before{
    content:"";
    width:20rpx;
    height:20rpx;
    display:inline-block;
    background:url(data:image/png;base64,);
    background-size: 20rpx 20rpx;
    margin:0 5rpx -1rpx;
}
.playNum::before{
    background:url(data:image/png;base64,);
    background-size: 20rpx 20rpx;
}




/* 页脚 */
.footer{
    width:100%;
    height:160rpx;
    font-size: 24rpx;
    color: #BDBDBD;
    text-align:center;
}

.toFound{
    width:70%;
    height:100rpx;
    line-height:100rpx;
    margin:30rpx auto;
    color:#fff;
    background:#FF552E;
    font-size:28rpx;
    border-radius:100rpx;
}


/* 分享弹窗 */
.modal-mask{
    width:100%;
    height:100%;
    position: absolute;
    left:0;
    top:0;
    background:rgba(0,0,0,0.3);
    z-index:9;
}

.doWith{
    width:100%;
    height:320rpx;
    position: fixed;
    left:0;
    bottom:0;
    z-index:10;
    background-color:#f3f3f3;
}
.doWithBtns,.doWithCancel{
    width:100%;
    position: absolute;
    left:0;
}
.doWithCancel{
    bottom:0;
}
.doWithBtns{
    top: 0;
    height:300rpx;
}
.doWithBtns button{
    border-radius:0;
    border-bottom: 1rpx solid #e5e5e5;
}
.doWithBtns button,.doWithCancel{
    width:100%;
    height:100rpx;
    line-height: 100rpx;
    background: #fff;
    color:#000;
    font-size: 36rpx;
    text-align: center;
}