/* pages/detail/detail.wxss */
view, text{
    font-size:24rpx;
    color:#333;
    box-sizing:border-box;
}
page,.wrap{
    width:100%;
    position:relative;
    min-height:100%;
}
.wrap{
    display:flex;
    flex-direction:column;
}




/* 邀请卡片 */
.inviteBox{
    height:118rpx;
    width:100%;
    background: #333333;
    display:flex;
    position:relative;
    box-sizing:border-box;
    padding:19rpx 30rpx 23rpx;
}
.toGame,.toInvite,.toIndex{
    padding:8rpx 18rpx;
    line-height:64rpx;
    text-align:center;
    background:#FF552E;
    font-size: 28rpx;
    color: #FFFFFF;
    position:absolute;
    right:20rpx;
    top:19rpx;
}
.toIndex{
    right:220rpx;
}
.petAvatar{
    width:78rpx;
    height:78rpx;
    border-radius:50%;
    margin-right:20rpx;
    background:#fff;
}
.invetor{
    flex:1;
}
.title{
    font-size: 32rpx;
    color: #FFFFFF;
}
.slogen{
    font-size: 24rpx;
    color: #FFFFFF;
    line-height:40rpx;
}





/* 视频播放 */
.videoPlay{
    height:548rpx;
    width:100%;
}
.video{
    width:100%;
    height:100%;
}
