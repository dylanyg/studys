/* pages/my/my.wxss */
/* 页面 */
.mod-my{
  background-color:#f6f6f6;
  font-size:0;  
}
.row{
  display:flex;
  flex-flow:row nowrap;
}
.column{
  display:flex;
  flex-flow:column nowrap;
}
/* 我们名片 */
.mod-card-bg{
  height:400rpx;
  background-color:#FF2D73;
  position:relative;
}
.mod-card-bg::after{
    content:'';
    display:block;
    width:750rpx;
    height:63rpx;
    position:absolute;
    bottom:0;
    background:url(data:image/png;base64,) no-repeat;
    background-size:100% 100%;
}
.mod-my-card{
  height:400rpx;
  width:100%;
  position:absolute;
  top:0;
  left:0;
  text-align: center;
}

.mod-my-photo{
  width:100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background:#f3f3f3;
}
.mod-my-intro{
  margin-top:20rpx;
  text-align: center;
}
.mod-my-nickname{
  line-height:48rpx;
  font-size: 34rpx;
  color: #FFFFFF;
}
.mod-my-pet-count{
  font-size: 28rpx;
  color: #FFFFFF;
  margin-top:5rpx;
  text-align:center;
}

/* 我的列表 */
.mod-my-container{
  position:relative;
  top:-136rpx;
  padding:0 30rpx;
}
.mod-my-list{
  background: #FFFFFF;
  border-radius: 20rpx;
  padding:0 20rpx;
}
.mod-my-list-li{
  height:120rpx;
  box-sizing:border-box;
  border-bottom:1rpx solid #F3F3F3;
  padding:36rpx 32rpx;
  font-size: 34rpx;
  color: #333333;
  line-height: 48rpx;
  display:flex;
  align-items: center;
  justify-content: space-between;  
}
.mod-my-menu{
  display:inline-flex;
  align-items: center;
}
.mod-my-menu::before{
  display:inline-flex;
  content:'';
  width:46rpx;
  height:46rpx;
  background:url('data:image/png;base64,') no-repeat center;
  background-size: 100%;
  margin-right:26rpx;
}
.mod-my-menu-works::before{
  background:url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAC4AAAAuBAMAAACllzYEAAAALVBMVEX/VS4AAAD/VS//Vi//VzT/9fL/6OP/1Mv/vKz/n4n/hWn/cE//YT3/VzH///9JejN2AAAADnRSTlPqAJmJLP37+fby7+3r6sjYHfAAAABQSURBVDjLYxDEDgZO3FkJE5gAxY2UlBhQgZKSMlBcCYu40kgQ78Uh/vwAdvF32TjEny7ALv7uJS5x0szJJs2dvYMsnOkpjiud48wXgy3/AgBQ10f/bpl5hwAAAABJRU5ErkJggg==') no-repeat center;
  background-size:100%;
}
.mod-my-menu-sense::before{
  background:url('data:image/png;base64,') no-repeat center;
  background-size:100%;
}
.mod-my-menu-prize::before{
  background:url('data:image/png;base64,') no-repeat center;
  background-size:100%;
}
.mod-my-menu-money::before{
  background:url('data:image/png;base64,') no-repeat center;
  background-size:100%;
}
.mod-my-menu-msg::before{
  background:url('data:image/png;base64,') no-repeat center;
  background-size:100%;
}
.mod-my-menu-setting::before{
  background:url('data:image/png;base64,') no-repeat center;
  background-size:100%;
}

.mod-my-list-li::after{
  display:inline-flex;
  content:'';
  width:15rpx;
  height:26rpx;
  background:url('https://static.58.com/lbg/mengchong/image/icon/arrow-right.png') no-repeat center;
  background-size: 100%;
  margin-right:26rpx;
}
.mod-my-list-li-last{
  border-bottom:none;
}
/* 设置 */
.mod-my-setting{
  margin-top:34rpx;
  border-radius: 20rpx;
  padding:0 20rpx;
  background-color:#fff;
}
.my-message-count{
  line-height:34rpx;
  font-size:24rpx;
  color:#FFFFFF;
  background:#FF552E;
  border-radius:17rpx;
  height:34rpx;
  padding:0 10rpx;
  position:absolute;
  right:150rpx;
}