view{
    box-sizing: border-box;
    font-weight: 300;
}

page {
    background-color: #eee;
    font-weight: 100;
    font-family: '"微软雅黑",arial,Tahoma,SimSun,sans-serif';
    font-weight: 300;
}

.detail-container {
    height: calc(100vh - 100rpx);
}

.center{
    text-align: center;
}

.mod-head{
    width: 690rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
    font-size: 34rpx;
    margin-bottom: 24rpx;
    overflow: hidden;
}

.mod-more{
    text-align: center;
    font-size: 28rpx;
    height: 88rpx;
    line-height: 88rpx;
    color: #999;
    border-top:2rpx solid #F3F3F3;
}

.mod-more::after{
    display: inline-block;
    content: '';
    width: 14rpx;
    height: 14rpx;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 14rpx 14rpx;
    margin: 0 0 4rpx 16rpx;
}

.no-padding {
    padding: 0;
}

.no-margin {
    margin: 0;
}

/* common */
.star-view{
    margin-top: 6rpx;
    line-height: 26rpx;
}

.star-red{
    background: url("data:image/png;base64,") no-repeat;
}
.star-gray{
    background: url("data:image/png;base64,") no-repeat;
}
.star-red, .star-gray{
    width: 24rpx;
    height: 24rpx;
    background-size: 24rpx;
    margin-right: 10rpx;
    display: inline-block;
}

.mod-price{
    color: #FF552E;
    font-size: 34rpx;
    height: 48rpx;
    line-height: 48rpx;
}

.mod-price1{
    color: #aaa;
    font-size: 24rpx;
    line-height: 33rpx;
    margin-left: 8rpx;
    text-decoration: line-through;
}
